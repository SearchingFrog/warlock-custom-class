<?xml version="1.0"?>
<GConfig>
  <config>
    <height>525</height>
    <width>700</width>
    <colwidth>325</colwidth>
    <title>PicLock Configuration</title>
    <tabs>
      <tab>Casting</tab>
      <tab>Demonology</tab>
    </tabs>
  </config>
	<input>
		<label> Maximum Casting Range</label>
		<target>PicLock.PULL_DISTANCE</target>
	</input>
	<input>
		<label> Spell Lead Delay</label>
		<target>PicLock.SPELL_DELAY</target>
	</input>
	<input>
		<label> Emergency Health %</label>
		<target>PicLock.EMERGENCY_HEALTH</target>
	</input>
	<input>
		<label> Maintain Health %</label>
		<target>PicLock.MIN_HEALTH_PCT</target>
	</input>
	<input>
		<label> Maintain Mana %</label>
		<target>PicLock.MIN_MANA_PCT</target>
	</input>
	<input>
		<label> Cast Until Targets Health Is Less Than %</label>
		<target>PicLock.MIN_MOB_HEALTH_PCT</target>
	</input>
	<input>
		<label> Maximum Amount Of Soul Shards To Carry</label>
		<target>PicLock.SOULSHARDS_MAX</target>
	</input>
	<input>
		<label> % Of Targets Health to Cast Drain Soul</label>
		<target>PicLock.DRAINSOUL_PCT</target>
		<tabindex>2</tabindex>
	</input>
	<checkbox>
		<label> Use Food</label>
		<target>PicLock.USE_FOOD</target>
	</checkbox>
	<checkbox>
		<label> Use Drink</label>
		<target>PicLock.USE_DRINK</target>
	</checkbox>
	<checkbox>
		<label> Sit If No Food Is Avalible</label>
		<target>PicLock.SIT_TO_REGEN</target>
	</checkbox>
	<checkbox>
		<label> Use Health Potions</label>
		<target>PicLock.USE_HEALTHPOTION</target>
	</checkbox>
	<checkbox>
		<label> Use Healthstone</label>
		<target>PicLock.USE_HEALTHSTONE</target>
	</checkbox>
	<checkbox>
		<label> Use Fear in PVP </label>
		<target>PicLock.USE_FEAR</target>
	</checkbox>
	<checkbox>
		<label> Deal with Adds </label>
		<target>PicLock.DEAL_WITH_ADDS</target>
	</checkbox>
	<checkbox>
		<label> Fight Back Against Players</label>
		<target>PicLock.FIGHT_BACK</target>
	</checkbox>
	<checkbox>
		<label> Initiate PVP</label>
		<target>PicLock.INITIATE_PVP</target>
	</checkbox>
  <input>
    <label> +/- Levels (Level Range for PvP)</label>
    <target>PicLock.LEVEL_DIFFERENCE</target>
  </input>

	<combobox>
		<label>Mount - Between Targets (BTM)</label>
		<target>PicLock.TravelForm</target>
		<options>
			<option>None</option>
			<option>Delay Mount</option>
			<option>Distance Mount</option>
		</options>
	</combobox>
	<input>
		<label> Delay In (s) OR Distance In (yds) for BTM</label>
		<target>PicLock.TravelFormDelay</target>
	</input>
	<longinput>
		<label>Mount Name</label>
		<target>PicLock.MountName</target>
	</longinput>






	
	<combobox>
		<label> Primary Attack</label>
		<target>PicLock.PRIMARY_ATTACK</target>
		<tabindex>2</tabindex>
		<options>
			<option>Drain Life</option>
			<option>Wand</option>
			<option>Shadow Bolt</option>
		</options>
	</combobox>
	<combobox>
		<label> Last Resort Attack</label>
		<target>PicLock.LAST_RESORT</target>
		<tabindex>2</tabindex>
		<options>
			<option>Wand</option>
			<option>Melee</option>
			<option>None</option>
		</options>
	</combobox>

	<checkbox>
		<label>Randomize Casting</label>
		<target>PicLock.USE_SPELLSHUFFLE</target>
		<tabindex>2</tabindex>
	</checkbox>
	<checkbox>
		<label> Use Blood Fury when able</label>
		<target>PicLock.USE_BLOODFURY</target>
		<tabindex>2</tabindex>
	</checkbox>
	<checkbox>
		<label> Use Armor Buff</label>
		<target>PicLock.USE_ARMORBUFF</target>
		<tabindex>2</tabindex>
	</checkbox>
	<checkbox>
		<label> Use Nightfall When Activated</label>
		<target>PicLock.USE_NIGHTFALL</target>
		<tabindex>2</tabindex>
	</checkbox>
	<checkbox>
		<label> Use Amplify Curse</label>
		<target>PicLock.USE_AMPLIFYCURSE</target>
		<tabindex>2</tabindex>
	</checkbox>
	<checkbox>
		<label> Use Drain Life</label>
		<target>PicLock.USE_DRAINLIFE</target>
		<tabindex>2</tabindex>
	</checkbox>
	<input>
		<label> % Of Your Health Before Drain Life</label>
		<target>PicLock.DRAINLIFE_PCT</target>
		<tabindex>2</tabindex>
	</input>
	<checkbox>
		<label> Use Drain Mana</label>
		<target>PicLock.USE_DRAINMANA</target>
		<tabindex>2</tabindex>
	</checkbox>
	<input>
		<label> % Of Your Mana Before Drain Mana</label>
		<target>PicLock.DRAINMANA_PCT</target>
		<tabindex>2</tabindex>
	</input>
	<checkbox>
		<label> Use Life Tap </label>
		<target>PicLock.USE_LIFETAP</target>
		<tabindex>2</tabindex>
	</checkbox>

  <checkbox>
    <label> Combat: Unstable Affliction </label>
    <target>PicLock.USE_UNSTABLEAFFLICTION</target>
    <tabindex>2</tabindex>
  </checkbox>
  
  <checkbox>
    <label> Combat: Corruption </label>
    <target>PicLock.USE_CORRUPTION</target>
    <tabindex>2</tabindex>
  </checkbox>
  
  <checkbox>
    <label> Combat: Curse of Agony </label>
    <target>PicLock.USE_CURSEOFAGONY</target>
    <tabindex>2</tabindex>
  </checkbox>
  
  <checkbox>
    <label> Combat: Siphon Life </label>
    <target>PicLock.USE_SIPHONLIFE</target>
    <tabindex>2</tabindex>
  </checkbox>

  <checkbox>
    <label> Combat: Immolate </label>
    <target>PicLock.USE_IMMOLATE</target>
    <tabindex>2</tabindex>
  </checkbox>

  <checkbox>
    <label> Combat: Conflagurate </label>
    <target>PicLock.USE_CONFLAGURATE</target>
    <tabindex>2</tabindex>
  </checkbox>
  
    <checkbox>
    <label> Combat: Incinerate </label>
    <target>PicLock.USE_INCINERATE</target>
    <tabindex>2</tabindex>
  </checkbox>
  
  <checkbox>
    <label> Combat: Shadow Bolt </label>
    <target>PicLock.USE_SHADOWBOLT</target>
    <tabindex>2</tabindex>
  </checkbox>

  <checkbox>
    <label> Emergency Combat: Death Coil </label>
    <target>PicLock.USE_DEATHCOIL</target>
    <tabindex>2</tabindex>
  </checkbox>


    
  <combobox>
		<label> Summon Demon</label>
		<target>PicLock.SUMMON_DEMON</target>
		<tabindex>3</tabindex>
		<options>
			<option>None</option>
			<option>Imp</option>
			<option>Voidwalker</option>
			<option>Succubus</option>
			<option>Felguard</option>
			<option>Felhunter</option>

		</options>
	</combobox>

	<checkbox>
		<label> Pet To Attack</label>
		<target>PicLock.PET_ATTACK</target>
		<tabindex>3</tabindex>
	</checkbox>
	<checkbox>
		<label> Wait For Pet To Gain Aggro</label>
		<target>PicLock.WAIT_FOR_PET_AGGRO</target>
		<tabindex>3</tabindex>
	</checkbox>
  <input>
    <label> Seconds to wait for Pet to gain aggro </label>
    <target>PicLock.PET_AGGRO_DELAY</target>
    <tabindex>3</tabindex>
  </input>
  <checkbox>
		<label> Resummon Pet In Combat</label>
		<target>PicLock.RESUMMON_IN_COMBAT</target>
		<tabindex>3</tabindex>
	</checkbox>
	<checkbox>
		<label> Use Soul Link</label>
		<target>PicLock.USE_SOULLINK</target>
		<tabindex>3</tabindex>
	</checkbox>
	<checkbox>
		<label> Use DarkPact</label>
		<target>PicLock.USE_DARKPACT</target>
		<tabindex>3</tabindex>
	</checkbox>
	<checkbox>
		<label> Use Fel Domination</label>
		<target>PicLock.USE_FELDOMINATION</target>
		<tabindex>3</tabindex>
	</checkbox>

	<checkbox>
		<label> Use Health Funnel</label>
		<target>PicLock.USE_HEALTHFUNNEL</target>
		<tabindex>3</tabindex>
	</checkbox>
	<input>
		<label> % Pet's Health Before Health Funnel Is Cast</label>
		<target>PicLock.HEALTHFUNNEL_PCT</target>
		<tabindex>3</tabindex>
	</input>
	<checkbox>
		<label> Use Consume Shadows</label>
		<target>PicLock.USE_CONSUMESHADOWS</target>
		<tabindex>3</tabindex>
	</checkbox>
	<input>
		<label>Voidwalker's Health% before Cosumeshadows Cast</label>
		<target>PicLock.CONSUMESHADOWS_PCT</target>
		<tabindex>3</tabindex>
	</input>






</GConfig>
